                    <sec2:client-auth-methods>
                        <ctls1:tlscert allows-self-signed="true"/>
                        <csig1:httpsig/>
                    </sec2:client-auth-methods>
                    <sec2:server-auth-methods>
                        <stls1:tlscert/>
                        <ssig1:httpsig/>
                    </sec2:server-auth-methods>
                </i2m:http-security>
                <i2m:url>https://aec-ewp.eu/v1/institutions</i2m:url>
                <i2m:max-hei-ids>15</i2m:max-hei-ids>
            </i2m:institutions>
