                <las1m:max-omobility-ids>1</las1m:max-omobility-ids>
            </las1m:omobility-las>
            <lacnr1m:omobility-la-cnr version="1.0.0">
                <lacnr1m:http-security>
                    <sec2:client-auth-methods>
