            <d5m:discovery version="5.0.0">
                <d5m:url>https://aec-ewp.eu/v1/manifest</d5m:url>
            </d5m:discovery>
            <e2m:echo version="2.0.1">
                <e2m:http-security>
                    <sec2:client-auth-methods>
                        <ctls1:tlscert allows-self-signed="true"/>
                        <csig1:httpsig/>
