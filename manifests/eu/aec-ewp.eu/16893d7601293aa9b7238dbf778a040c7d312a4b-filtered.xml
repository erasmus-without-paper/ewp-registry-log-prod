                        <csig1:httpsig/>
                    </sec2:client-auth-methods>
                    <sec2:server-auth-methods>
                        <stls1:tlscert/>
                        <ssig1:httpsig/>
                    </sec2:server-auth-methods>
                </ou2m:http-security>
                <ou2m:url>https://aec-ewp.eu/v1/organizational-units</ou2m:url>
                <ou2m:max-ounit-ids>15</ou2m:max-ounit-ids>
                <ou2m:max-ounit-codes>15</ou2m:max-ounit-codes>
            </ou2m:organizational-units>
            <las1m:omobility-las version="1.0.0">
                <las1m:http-security>
                    <sec2:client-auth-methods>
                        <ctls1:tlscert allows-self-signed="true"/>
                        <csig1:httpsig/>
                    </sec2:client-auth-methods>
                    <sec2:server-auth-methods>
                        <stls1:tlscert/>
                        <ssig1:httpsig/>
                    </sec2:server-auth-methods>
                </las1m:http-security>
                <las1m:get-url>https://aec-ewp.eu/v1/outgoing-mobility-learning-agreement/get</las1m:get-url>
                <las1m:index-url>https://aec-ewp.eu/v1/outgoing-mobility-learning-agreement/index</las1m:index-url>
