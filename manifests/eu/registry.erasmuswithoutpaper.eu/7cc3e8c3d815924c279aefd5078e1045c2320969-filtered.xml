<ns3:manifest
    xmlns:ns3="https://github.com/erasmus-without-paper/ewp-specs-api-discovery/tree/stable-v6"
    xmlns="https://github.com/erasmus-without-paper/ewp-specs-architecture/blob/stable-v1/common-types.xsd"
    xmlns:ns2="https://github.com/erasmus-without-paper/ewp-specs-api-registry/tree/stable-v1"
>
    <ns3:host>
        <admin-email>ewp-tech@lists.erasmuswithoutpaper.eu</admin-email>
        <admin-provider>EWP</admin-provider>
        <admin-notes>This host handles the EWP Registry Service.</admin-notes>
        <ns2:apis-implemented>
            <discovery
                xmlns="https://github.com/erasmus-without-paper/ewp-specs-api-discovery/tree/stable-v6/manifest-entry.xsd"
                version="6.0.0"
            >
                <url>https://registry.erasmuswithoutpaper.eu/manifest-registry.xml</url>
            </discovery>
            <registry
                xmlns="https://github.com/erasmus-without-paper/ewp-specs-api-registry/blob/stable-v1/manifest-entry.xsd"
                version="1.3.0"
            >
                <catalogue-url>https://registry.erasmuswithoutpaper.eu/catalogue-v1.xml</catalogue-url>
            </registry>
        </ns2:apis-implemented>
    </ns3:host>
</ns3:manifest>
