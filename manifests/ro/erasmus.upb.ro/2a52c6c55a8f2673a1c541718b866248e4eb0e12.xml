                                xmlns="https://github.com/erasmus-without-paper/ewp-specs-sec-srvauth-httpsig/tree/stable-v1"/>
                    </sec:server-auth-methods>
                </http-security>
                <get-url>https://erasmus.upb.ro/dev/ewp/iias/get</get-url>
                <max-iia-ids>2</max-iia-ids>
                <index-url>https://erasmus.upb.ro/dev/ewp/iias/index</index-url>
                <stats-url>https://erasmus.upb.ro/dev/ewp/iias/stats</stats-url> <!-- TODO - parsing error -->
